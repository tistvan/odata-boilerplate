{
	"_version": "1.7.0",
	"sap.app": {
		"id": "de.blogspot.openui5.odata.explorer",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"catalogService": {
				"uri": "/sap/opu/odata/iwfnd/catalogservice/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "de.blogspot.openui5.odata.explorer.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {},
				"sap.ui.layout": {},
				"sap.uxap": {},
				"sap.f": {},
				"sap.suite.ui.commons": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"xmodels": {
			"catalog": {
				"dataSource": "catalogService",
				"settings": {
					"defaultCountMode": "Inline",
					"useBatch": false
				},
				"preload": true
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "de.blogspot.openui5.odata.explorer.i18n.i18n"
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "de.blogspot.openui5.odata.explorer.view",
				"controlId": "idAppControl",
				"controlAggregation": "pages",
				"async": true
			},
			"routes": [
				{
					"pattern": ":entityType:",
					"name": "metadata",
					"target": "metadata"
				}
			],
			"targets": {
				"metadata": {
					"viewName": "Metadata",
					"viewId": "metadata",
					"viewLevel": 1
				},
				"visualizer": {
					"viewName": "MetadataVis",
					"viewId": "visualizer",
					"viewLevel": 2
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}